@model ProjectListViewModel
    
    <div class="container mt-5 mb-5">
        <header>
        <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
            <div class="navbar-collapse" id="navbarСollapse">
                <ul class="navbar-nav mr-auto">

                    <li class="nav-item active">
                        <a class="nav-link">Главная</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="#">Управление пользователями</a>
                    </li>

                </ul>

                <form class="form-inline mt-2 mt-md-0">
                    <a class="nav-link" href="/Account/Logout">Выйти</a>
                </form>
            </div>
        </nav> 
        
    </header>


        <hr />
        <hr />
        <div class="currencies float-left">
            <h2>
                Все проекты
            </h2>

        </div>
        <hr />
        <hr />
        <hr />
        <style>
            button {
                border: 2px solid #e34761; /* Параметры рамки */
                background: #f3dbdf; /* Цвет фона */
                font-size: 14px; /* Размер текста */
                padding: 5px 20px; /* Поля вокруг текста */
            }
        </style>

        @foreach (var el in Model.ProjectModels.Projects)
        {
            <td>Название проекта: @el.Name</td>
            <br>
            <td>ID проекта: @el.Id</td>
            <br>
            <a class="" asp-controller="ProjectPage" asp-action="getProjectId" asp-route-id="@el.Id">Перейти на проект</a>
            <br>
            <hr />
        }
        <hr />
        <hr />
        <a class="btn badge-light border-dark" href="/Project/Presentation">Добавить проект</a>

    </div>
