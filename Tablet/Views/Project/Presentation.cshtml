@model Project

<div class="row  mt-5 mb-5">
    <hr />
    <hr />
    <div class="currencies float-left">
        <h2>
            Добавление значения
        </h2>

    </div>
    <hr />
    <hr />
    <hr />
    <form asp-action="Presentation" method="post" class="form">
        <div class="form-group">
            <label asp-for="Name"></label>
            <div class="col-md-5">
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Customer"></label> Forecast
            <div class="col-md-5">
                <input asp-for="Customer" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Developer"></label>
            <div class="col-md-5">
                <input asp-for="Developer" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Technology"></label>
            <div class="col-md-5">
                <input asp-for="Technology" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Cost"></label>
            <div class="col-md-5">
                <input asp-for="Cost" class="form-control" />
            </div>
        </div>
        <input class="btn btn-success" type="submit" value="Добавить в таблицу" />
    </form>
</div>